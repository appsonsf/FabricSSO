<?xml version="1.0" encoding="utf-8" ?>
<Settings xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://schemas.microsoft.com/2011/01/fabric">
  <!-- Add your custom configuration sections and parameters here -->
  <!--
  <Section Name="MyConfigSection">
    <Parameter Name="MyParameter" Value="Value1" />
  </Section>
  -->
  <Section Name="ConnectionStrings">
    <Parameter Name="DefaultConnection" Value="Data Source=(local)\SQLEXPRESS;Initial Catalog=SsoSfApp_ConfigDb;Integrated Security=True;MultipleActiveResultSets=true" />
  </Section>

  <Section Name="Diagnostics">
    <!--Critial, Error, Warning, Informational, Verbose, LogAlways-->
    <Parameter Name="EventSourceLevel" Value="Informational" />
    <!-- 
        Verbose = 0,
        Debug = 1,
        Information = 2,
        Warning = 3,
        Error = 4,
        Fatal = 5
    -->
    <Parameter Name="SerilogEventLevel" Value="Debug" />
    <Parameter Name="ElasticsearchHost" Value="http://127.0.0.1:9200" />
  </Section>

</Settings>
